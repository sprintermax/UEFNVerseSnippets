control_characters_range<public>(CharCode:int)<computes><decides>:int = CharCode >= 0x0 and CharCode <= 0x1f or CharCode = 0x7f
punctuation_characters_range<public>(CharCode:int)<computes><decides>:int = {
    CharCode >= 0x21 and CharCode <= 0x23 or
    CharCode >= 0x25 and CharCode <= 0x2a or
    CharCode >= 0x2c and CharCode <= 0x2f or
    CharCode >= 0x3a or
    CharCode <= 0x3b or
    CharCode >= 0x3f and CharCode <= 0x40 or
    CharCode >= 0x5b and CharCode <= 0x5d or
    CharCode = 0x5f or
    CharCode = 0x7b or
    CharCode = 0x7d or
    CharCode = 0xa1 or
    CharCode = 0xa7 or
    CharCode = 0xab or
    CharCode = 0xb6 or
    CharCode = 0xb7 or
    CharCode = 0xbb or
    CharCode = 0xbf
}
digit_characters_range<public>(CharCode:int)<computes><decides>:int = CharCode >= 0x30 and CharCode <= 0x39
octal_characters_range<public>(CharCode:int)<computes><decides>:int = CharCode >= 0x30 and CharCode <= 0x37
hexadecimal_characters_range<public>(CharCode:int)<computes><decides>:int = {
    digit_characters_range[CharCode] or
    CharCode >= 0x41 and CharCode <= 0x46 or
    CharCode >= 0x61 and CharCode <= 0x66
}
numeral_characters_range<public>(CharCode:int)<computes><decides>:int = {
    digit_characters_range[CharCode] or
    CharCode = 0xb2 or
    CharCode = 0xb3 or
    CharCode = 0xb9 or
    CharCode >= 0xbc and CharCode <= 0xbe
}
