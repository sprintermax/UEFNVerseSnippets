printable_characters_range<public>(CharCode:int)<computes><decides>:int = {
    CharCode >= 0x20 and CharCode <= 0x7e or
    CharCode >= 0xa1 and CharCode <= 0xac or
    CharCode >= 0xae and CharCode <= 0xff
}
graphical_characters_range<public>(CharCode:int)<computes><decides>:int = {
    printable_characters_range[CharCode] or spacing_characters_range[CharCode]
}
alphabet_characters_range<public>(CharCode:int)<computes><decides>:int = {
    uppercase_characters_range[CharCode] or
    lowercase_characters_range[CharCode]
}

ascii_characters_range<public>(CharCode:int)<computes><decides>:int = CharCode >= 0x20 and CharCode <= 0x7e
