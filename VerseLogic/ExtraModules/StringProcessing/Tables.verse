lowercase_characters_range<public>(CharCode:int)<computes><decides>:int = {
    CharCode >= 0x61 and CharCode <= 0x7a or
    CharCode = 0xb5 or
    CharCode >= 0xdf and CharCode <= 0xf6 or
    CharCode >= 0xf8 and CharCode <= 0xff
